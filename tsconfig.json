{
  "extends": "./node_modules/astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    // "strictNullChecks": true,
    "baseUrl": ".",
    "strict": false,
    "types": ["@types/vue"],
    "paths": {
      "@/components/*": ["src/components/*"],
      "@/layouts/*": ["src/layouts/*"],
      "@/pages/*": ["src/pages/*"],
      "@/utils/*": ["src/utils/*"],
      "@/styles/*": ["src/styles/*"],
      "@/content/*": ["src/content/*"]
    }
  }
}
